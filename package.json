{
  "name": "diggin-it",
  "version": "1.0.14",
  "description": "7DRL 2021",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "start": "rollup -cw",
    "lint": "eslint --fix src",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "butler:web": "butler push --userversion 1.0.14 dist lagdotcom/diggin-it:web",
    "status:web": "butler status lagdotcom/diggin-it:web",
    "web": "npm run build && npm run butler:web",
    "resources:win": "shx cp dist/*.* DigginItWinWrap/Resources",
    "version:win": "node --loader ts-node/esm src/scripts/winversions.ts",
    "update:win": "cd DigginItWinWrap && nuget restore && nuget update DigginItWinWrap.sln",
    "build:win": "msbuild -m -p:Configuration=Release DigginItWinWrap/DigginItWinWrap.sln",
    "butler:win": "butler push --userversion 1.0.14 DigginItWinWrap/bin/Release lagdotcom/diggin-it:win",
    "status:win": "butler status lagdotcom/diggin-it:win",
    "prepare:win": "npm run build && npm run resources:win && npm run version:win && npm run build:win",
    "win": "npm run prepare:win && npm run butler:win",
    "bean:fresh": "curl https://justine.lol/redbean/redbean-latest.com >redbean.com",
    "bean": "zip -uj redbean.com .init.lua dist/*"
  },
  "author": "Lag.Com <lagdotcom@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@lagdotcom/simple-inputs": "git+ssh://github.com/lagdotcom/simple-inputs.git",
    "bresenham": "0.0.4",
    "rot-js": "git+ssh://github.com/lagdotcom/rot.js.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-url": "^6.1.0",
    "@types/assert": "^1.5.5",
    "@types/bresenham": "0.0.0",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "assert": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-woke": "^1.0.0",
    "esm": "^3.2.25",
    "mocha": "^9.0.3",
    "prettier": "^2.3.2",
    "rollup": "^2.56.2",
    "rollup-plugin-import-file": "^1.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagdotcom/diggin-it.git"
  },
  "bugs": {
    "url": "https://github.com/lagdotcom/diggin-it/issues"
  },
  "homepage": "https://github.com/lagdotcom/diggin-it#readme"
}
