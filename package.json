{
  "name": "diggin-it",
  "version": "1.0.8",
  "description": "7DRL 2021",
  "main": "src/index.ts",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "start": "rollup -cw",
    "lint": "eslint --fix src",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "butler:web": "butler push dist lagdotcom/diggin-it:web",
    "status:web": "butler status lagdotcom/diggin-it:web",
    "web": "npm run build && npm run butler:web",
    "resources:win": "copy dist\\*.* DigginItWinWrap\\Resources",
    "version:win": "node --loader ts-node/esm src/scripts/winversions.ts",
    "update:win": "cd DigginItWinWrap && nuget restore && nuget update DigginItWinWrap.sln",
    "build:win": "msbuild -p:Configuration=Release DigginItWinWrap\\DigginItWinWrap.sln",
    "butler:win": "butler push --userversion 1.0.8 DigginItWinWrap\\bin\\Release lagdotcom/diggin-it:win",
    "status:win": "butler status lagdotcom/diggin-it:win",
    "win": "npm run build && npm run resources:win && npm run version:win && npm run build:win && npm run butler:win"
  },
  "author": "Lag.Com <lagdotcom@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@lagdotcom/simple-inputs": "git+https://github.com/lagdotcom/simple-inputs.git",
    "rot-js": "git+https://github.com/lagdotcom/rot.js.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@types/assert": "^1.5.4",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "assert": "^2.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-woke": "^1.0.0",
    "esm": "^3.2.25",
    "mocha": "^9.0.0",
    "prettier": "^2.3.1",
    "rollup": "^2.51.2",
    "rollup-plugin-import-file": "^1.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lagdotcom/diggin-it.git"
  },
  "bugs": {
    "url": "https://github.com/lagdotcom/diggin-it/issues"
  },
  "homepage": "https://github.com/lagdotcom/diggin-it#readme"
}
